{
  "editor.fontSize": 13,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "tabnine.experimentalAutoImports": true,
  "autoimport.showNotifications": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.colorTheme": "GitHub Light Default",
  "workbench.iconTheme": "vscode-icons",
  "autoimport.doubleQuotes": false,
  "autoimport.filesToScan": "**/*.{ts,tsx.js,jsx}",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "inline-parameters.hideRedundantAnnotations": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "git.suggestSmartCommit": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "python.languageServer": "Pylance",
  "gitlens.gitCommands.closeOnFocusOut": true,
  "problems.showCurrentInStatus": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 30000,
  "eslint.format.enable": true,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": null,
  "editor.formatOnType": true,
  "sync.autoUpload": true,
  "sync.forceUpload": true,
  "sync.autoDownload": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.wrapTabs": true,
  "editor.parameterHints.cycle": true,
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    "DEBUG",
    "REVIEW",
    {
      "text": "TODO",
      "color": "red",
      "border": "1px solid red",
      "borderRadius": "2px" //NOTE: using borderRadius along with `border` or you will see nothing change
      //other styling properties goes here ...
    }
  ],
  "todohighlight.keywordsPattern": "TODO|FIXME", // TODO|FIXME|    highlight `TODO`,`FIXME` or content between parentheses : \\(([^)]+)\\)

  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "todohighlight.maxFilesForSearch": 5000,
"todohighlight.toggleURI": false,
  "parameterHints.enabled": false,
  "kite.showWelcomeNotificationOnStartup": false,
  "sonarlint.rules": {
    "javascript:S108": {
      "level": "off"
    }
  },
  "yaml.schemas": {
    "file:///Users/r/.vscode/extensions/atlassian.atlascode-2.8.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.jira.enabled": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "redhat.telemetry.enabled": true,
  "json.maxItemsComputed": 25000,
  "outline.showVariables": false,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "editor.largeFileOptimizations": false,
  "terminal.integrated.inheritEnv": false,
  "workbench.editor.untitled.hint": "hidden",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "editor.inlineSuggest.enabled": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true
  },
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "json.schemas": [
    {
      "name": "vscode-expo-manifest",
      "url": "file:///Users/r/Library/Application%20Support/Code/User/globalStorage/bycedric.vscode-expo/manifest-45.0.0.json",
      "fileMatch": [
        "app.json",
        "app.config.json"
      ]
    }
  ],
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "bracketPairColorizer.depreciation-notice": false,
  "cSpell.language": "en,ru",

}