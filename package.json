  "scripts": {
    "md-lint": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "md-lint-fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\" \"#venv\" ",
    "hugo-lunr-ml": "hugo-lunr-ml",
    "pull-git-modules": "git submodule update --init --recursive",
    "upd-modules": "git submodule update --remote --merge",
    "push": "git add .; git commit -am upd; git push",
    "p": "npm i; npm audit fix; npm run md-lint-fix; git commit -am upd; git pull; npm run upd-modules; npm run push"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "dependencies": {
    "html-react-parser": "^5.0.1",
    "marked": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3",
    "hugo-lunr-ml": "^3.1.1",
    "markdownlint-cli2": "^0.12.1",
    "postcss": "^8.4.28",
    "postcss-cli": "10.1.0",
    "postcss-import": "^16.0.0",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "tailwindcss": "^3.3.3",
    "tailwindcss-rtl": "^0.9.0"
  }
